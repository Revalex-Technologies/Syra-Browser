{
  "name": "syra",
  "version": "1.0.0",
  "packageManager": "yarn@4.10.3",
  "sideEffects": false,
  "description": "fast lightwaight and extendable.",
  "keywords": [
    "web-browser",
    "material",
    "electron",
    "react",
    "mobx",
    "styled-components"
  ],
  "homepage": "https://github.com/Revalex-Technologies/Syra-Browser#readme",
  "main": "build/main.bundle.js",
  "author": "Revalex Technologies <revalextechnologies@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Revalex-Technologies/Syra-Browser.git"
  },
  "bugs": {
    "url": "https://github.com/Revalex-Technologies/Syra-Browser/issues"
  },
  "scripts": {
    "dev-renderer": "cross-env DEV=1 webpack serve --config webpack.config.renderer.js",
    "dev-webpack": "cross-env DEV=1 webpack",
    "build-renderer": "cross-env webpack --config webpack.config.renderer.js",
    "dev": "cross-env START=1 npm run watch",
    "build": "rimraf build && concurrently \"npm run build-renderer\" \"cross-env webpack\"",
    "ci-build": "node scripts/ci-build.js",
    "start": "electron .",
    "watch": "concurrently \"npm run dev-renderer\" \"npm run dev-webpack\"",
    "compile-win32": "npm run build && electron-builder -w",
    "compile-darwin": "npm run build && electron-builder -m",
    "compile-linux": "npm run build && electron-builder -l",
    "lint": "eslint \"src/**/*.ts*\" \"src/**/*.tsx*\"",
    "lint-fix": "npm run lint -- --fix",
    "rebuild": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@electron/remote": "^2.1.3",
    "@emotion/is-prop-valid": "^1.4.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@types/animejs": "^3.1.13",
    "@types/chrome": "0.1.24",
    "@types/crypto-js": "^4.2.2",
    "@types/jszip": "^3.4.1",
    "@types/node": "^24.0.0",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.25",
    "@types/rimraf": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "8.40.0",
    "@typescript-eslint/parser": "8.40.0",
    "@wexond/rpc-core": "^1.0.3",
    "@wexond/rpc-electron": "^1.0.3",
    "animejs": "^4.2.2",
    "awesome-node-loader": "^1.1.1",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.2.0",
    "copy-webpack-plugin": "^13.0.1",
    "cross-env": "10.1.0",
    "electron": "^38.4.0",
    "electron-builder": "26.0.12",
    "electron-updater": "6.6.2",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "file-type": "^21.0.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "html-webpack-plugin": "^5.6.4",
    "jszip": "^3.10.1",
    "mobx": "6.15.0",
    "mobx-react-lite": "4.1.1",
    "node-bookmarks-parser": "^2.0.0",
    "prettier": "3.6.2",
    "pretty-bytes": "7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.0.0",
    "react-refresh": "^0.18.0",
    "react-windows-controls": "^1.1.1",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "styled-components": "^6.1.19",
    "stylis": "^4.3.6",
    "terser": "^5.44.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "typescript-plugin-styled-components": "^3.0.0",
    "webpack": "^5.102.1",
    "webpack-bundle-analyzer": "^4.10.2",
