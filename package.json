    "electron-chrome-web-store": "^0.13.0",
    "electron-store": "^11.0.2"
  },
  "resolutions": {
    "@types/react": "18.2.74",
    "@types/react-dom": "18.2.25",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
